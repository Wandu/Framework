{
    "name": "wandu/router",
    "description": "FastRouter with PSR-7 Wrapper Library.",
    "keywords": ["wandu", "http", "router", "psr-7", "psr7", "fast-router", "fast"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wandu\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wandu\\Router\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "psr/http-message": "^1",
        "nikic/fast-route": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "mockery/mockery": "0.9.*",
        "wandu/di": "^2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "suggest": {
        "wandu/http": "PSR-7 implementation",
        "wandu/di": "For Using WanduMapper"
    }
}
